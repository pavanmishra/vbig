<div class="t">&nbsp;</div>
<div class="c">
	<div class="title">
		<h2>New Contest</h2>
	</div><!--title-->
	<div class="search-form">
<% form_for(@contest, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </p>

  <p>
	<%= f.label :image, "Photo" %>
	<%= f.file_field :image %>
  </p>
	<%= hidden_field_tag :for_sponsor, params[:sponsor]%>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contests_path %>
	</div>
</div>
<div class="b">&nbsp;</div>
</div>
